[
  {
    "name": "UU看书网",
    "url": "http://sj.uukanshu.com",
    "textEncode": "gb2312",
    "capture": {
      "catalogScript": "document.location.href",
      "nameScript": "var returnTitle = null;var h1s = document.getElementsByClassName('bookname');if (h1s && h1s[0]) { returnTitle = h1s[0].innerText;}\nreturnTitle"
    },
    "catalog": {
      "query": "//div[@class='ml-list']/ul[@id='chapterList']/li",
      "reverse": false
    },
    "page": {
      "previousTitle": "[上一章]",
      "previousLinkQuery": "//div[@id='read-page']/div[@class='rp-switch']/a[@id='read_pre']",
      "nextTitle": "[下一章]",
      "titleQuery": "//div[@id='divContent']/h3",
      "nextLinkQuery": "//div[@id='read-page']/div[@class='rp-switch']/a[@id='read_next']",
      "contentQuery": "//div[@id='read-page']/div[@id='bookContent']"
    }
  },
  {
    "name": "爱下电子书",
    "url": "https://www.aixdzs.com",
    "textEncode": "utf-8",
    "pcVersion": true,
    "capture": {
      "catalogScript": "window.location.href",
      "nameScript": "var divs = document.getElementsByClassName('d_info'); if (divs && divs[0]) { var h1s = divs[0].getElementsByTagName('h1'); h1s[0].innerText }"
    },
    "catalog": {
      "query": "//div[@class='catalog']/ul/li[@class='chapter']"
    },
    "page": {
      "previousTitle": "[←]上一章",
      "previousLinkQuery": "//div[@class='link']/a",
      "nextTitle": "下一章[→]",
      "titleQuery": "//div[@class='line']/h1",
      "nextLinkQuery": "//div[@class='link']/a",
      "contentQuery": "//div[@class='content']"
    }
  }
]
